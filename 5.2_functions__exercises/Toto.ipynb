    "def flipcoin(reply):\n",
    "    if reply.lower == 'no':\n",
    "        return \"Okay good day sir\"\n",
    "    if reply.lower == 'yes':\n",
    "        sixnumbers = np.random.randint(1,101,6)\n",
    "        return sixnumbers\n",
    "\n",
    "try:\n",
    "    reply = input(\"Do you 6 numbers today? \")\n",
    "    fc = flipcoin(reply)\n",
    "    print(f\"{fc}\")\n",
    "\n",
    "\n",
    "except ValueError:\n",
    "    print(\"Please answer only yes or no\")"
   ]
